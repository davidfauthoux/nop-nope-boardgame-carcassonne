		<content>
			<span class="cc-setup">4 players</span>
		</content>
		<trigger>drop:setup_4_players</trigger>
		<script>
		<![CDATA[
			global.destroySetup(here);
			global.createPlayers(pools, tracks, 4);
			global.setupStartingTile(pools, grids);

			mark();
		]]>
		</script>
	</item>


	<item>
		<kind>watchdog_tile</kind>
		<trigger>watchdog:tile</trigger>
		<script>
		<![CDATA[
			/* No! or it will not move when the grid is extended
			if (
				(
					(_.action === "move")
					||
					((_.action === "paint") && (hover.look("face") !== undefined))
				)
				&&
				(hover.is("tile").split(/\-/g)[0].split(/\_/g)[1] === "000")
			) {
				throw "starting tile";
			}
			*/

			if ((_.action === "move") && (from.location === "pool-global") && (here.is("grid") !== undefined)) {
				throw "tile draw";
			}

			if (here.is("tile") !== undefined) {
				throw "tile in tile";
			}
		]]>
		</script>
	</item>
	<item>
		<kind>watchdog_random</kind>
		<trigger>watchdog:tileback</trigger>
		<script>
		<![CDATA[
			if (here.location !== "pool-global") {
				throw "random";
			}
		]]>
		</script>
	</item>
	<item>
		<kind>watchdog_live</kind>
		<trigger>watchdog:live</trigger>
		<script>
		<![CDATA[
			if (here.location !== from.location) {
				throw "live";
			}
		]]>
		</script>
	</item>
	<item>
		<kind>watchdog_audio</kind>
		<trigger>watchdog:audio</trigger>
		<script>
		<![CDATA[
			if ((_.action !== "destroy") && !here.live() && !hover.live()) {
				throw "!live";
			}
		]]>
		</script>
	</item>
	<item>
		<kind>watchdog_video</kind>
		<trigger>watchdog:video</trigger>
		<script>
		<![CDATA[
			if ((_.action !== "destroy") && !here.live() && !hover.live()) {
				throw "!live";
			}
		]]>
		</script>
	</item>


